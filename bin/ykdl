#!/usr/bin/env python
# -*- coding: utf-8 -*-

from argparse import ArgumentParser
import socket
import os
import sys
import json

sys.path = sys.path[1:]

try:
    import ykdl
except(ImportError):
    import sys
    _base_len = len('bin/ykdl')
    _filepath = sys.argv[0][:-_base_len]
    sys.path.insert(1, _filepath)
    import ykdl

from ykdl.__main__ import main

if __name__ == '__main__':
    main()
